<%= link_to "Back", @photo %>

<h1>Edit Photo</h1>

<%= form_for @photo do |f| %>
 <p>
   <%= f.label :title %>
   <%= f.text_field :title %>
 </p>
 <p>
   <%= f.label :author %>
   <%= f.text_field :author %>
 </p>
 <p>
   <%= f.label :url %>
   <%= f.text_field :url %>
 </p>
 <p>
   <%= f.submit %>
 </p>

<% end %>
